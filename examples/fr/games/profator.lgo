#---------- Contracteur ----------

pour contracter :sentence
contractera :sentence []
fin

pour contractera :versa :versb
donne "nvers compte :versa attends 2
si :nvers = 0 [contracter0 :versb [] stop]
si vide? :versa  [contracter0 :versb [] stop] [
si :nvers = 1 [contracter0 (md prem :versa :versb) [] stop ] [
donne "i prem :versa
donne "j item 2 :versa
si (compte :j) < 4 [contractera sp :versa md :i :versb] [
si (mot der sd :j der :j) = "_à [contractera mp "à sp sp :versa md (sd sd :j) md :i :versb] [
si (mot der sd sd :j der sd :j der :j) = "_de [contractera mp "de sp sp :versa md (sd sd sd :j) md :i :versb] [
si non vide? :versa [contractera sp :versa md :i :versb]]]]]]
fin

